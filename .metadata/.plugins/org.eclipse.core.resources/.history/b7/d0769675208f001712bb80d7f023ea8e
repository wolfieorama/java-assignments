import java.awt.Color;

public class WithinParkingTime
{
	
	ParkedCar car = new ParkedCar("BMW", "RAD 345T", Color.blue, 10);
	ParkingMeter pm = new ParkingMeter();
	PoliceOfficer police = new PoliceOfficer("Johnson", "JHPO");
//	ParkingTicket pt = new ParkingTicket(car, police);
	
	public String toString()
	{
		return "this is Car: " + car + "\n" +
				"this is Parking Meter: " + pm + "\n" +
				"this is Police: " + police;
//		"\n" +
//				"the cost of fine is: " + pt;
	}
}
