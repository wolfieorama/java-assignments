import java.awt.Color;

public class TestParkingTicket
{

	public static void main(String[] args)
	{
		ParkedCar car = new ParkedCar("BMW", "RAD 345T", Color.blue, 10);
		ParkingMeter pm = new ParkingMeter(20.0);
		PoliceOfficer police = new PoliceOfficer("Johnson", "JHPO");
		ParkingTicket pt = new ParkingTicket(car, police);
		
		System.out.println(car);
		System.out.println("========================");
		System.out.println(pm);
		System.out.println("========================");
		System.out.println(police);
		System.out.println("========================");

	}

}
